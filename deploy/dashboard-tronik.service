[Unit]
Description=Dashboard-TRONIK (Gunicorn)
After=network.target

[Service]
User=natanheringer
WorkingDirectory=/home/natanheringer/Documents/GitHub/dashboard-Tronik
Environment="PATH=/home/natanheringer/Documents/GitHub/dashboard-Tronik/.venv/bin"
EnvironmentFile=-/home/natanheringer/Documents/GitHub/dashboard-Tronik/.env
ExecStart=/home/natanheringer/Documents/GitHub/dashboard-Tronik/.venv/bin/gunicorn -c gunicorn.conf.py app:app
Restart=always
RestartSec=3

[Install]
WantedBy=multi-user.target


